    "typescript": "^5.7.3",
    "ultracite": "^4.1.20"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.4.1",
  "type": "module",
  "dependencies": {
    "tsup": "^8.3.6",
    "zod": "^3.24.2"
  }
}
