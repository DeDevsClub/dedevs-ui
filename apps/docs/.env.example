# Logo.dev API token for displaying provider logos
NEXT_PUBLIC_LOGO_DEV_TOKEN=

# Google Analytics measurement ID (format: G-XXXXXXXXXX)
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# Sentry DSN for error tracking
NEXT_PUBLIC_SENTRY_DSN=

# Pro product ID for subscription management
NEXT_PUBLIC_PRO_PRODUCT_ID=

# Source URL for component registry
NEXT_PUBLIC_SOURCE_URL=https://ui.dedevs.com

# Database connection string (PostgreSQL)
DATABASE_URL=

# Polar API access token for subscription management
POLAR_ACCESS_TOKEN=

# GitHub token for repository access
GITHUB_TOKEN=

# Base URL for the application
PORT=3422
BASE_URL=http://localhost:3422

# BetterStack monitoring (optional)
BETTERSTACK_API_KEY=
BETTERSTACK_URL=

# Sentry configuration (optional)
SENTRY_ORG=
SENTRY_PROJECT=

# OAuth providers (currently commented out in auth config)
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
# GITHUB_CLIENT_ID=
# GITHUB_CLIENT_SECRET=

# Build analysis (optional)
ANALYZE=false